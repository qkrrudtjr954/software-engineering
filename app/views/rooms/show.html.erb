<div class="room">
    <div class="row">
      <div class="panel panel-primary">
          <div class="panel-heading">
              <h3 class="panel-title">CSNS</h3>
          </div>
          <div class="panel-body">
              <div class="tab-content">
                  <div class="tab-pane active" id="messages">
                    <%= render @messages %>
                  </div>
              </div>
          </div>
      </div>
    </div>
</div>
<div class="footer">
  <div class="input-box">
    <input type="text" class="input-box_text" data-behavior="room_speaker"/>
  </div>
</div>
